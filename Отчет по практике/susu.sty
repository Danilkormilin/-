\newcommand{\sortitem}[3]{%
	\DTLnewrow{list}%
	\DTLnewdbentry{list}{label}{#1}%
	\DTLnewdbentry{list}{refer}{#2}%
	\DTLnewdbentry{list}{sorter}{#3}%
}

\newcounter{mycounter}
\setcounter{mycounter}{1}
\newenvironment{sortedlist_rus}
{% \begin{sortedlist}
	\DTLifdbexists{list}{\DTLcleardb{list}}{\DTLnewdb{list}}%
}{% \end{sortedlist}
	\DTLsort{sorter}{list}%
	\begin{enumerate}[start=\the\value{mycounter}, label={\arabic*}]%
		\DTLforeach*{list}{\theLabel=label, \theRefer=refer}{%
			\item \theLabel \label{\theRefer} \addtocounter{mycounter}{1}}%
	\end{enumerate}%
}

